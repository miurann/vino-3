  <%= form_with url: login_path, method: :post do |f| %>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.text_field :email, class: 'input form-control w-full bg-white my-3' %>
    </div>
    <div class="field">
      <%= f.label :password %><br />
      <%= f.password_field :password, class: 'input form-control w-full bg-white my-3'%>
    </div>
      <%= f.submit "Login", class: 'btn btn-secondary'%>
  <% end %>
